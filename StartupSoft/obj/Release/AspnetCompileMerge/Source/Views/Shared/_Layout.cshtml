<!DOCTYPE html>
<html ng-app="startupsoft">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="find a business partner, find a cofounder, find cofounder, Co-Founders Wanted, looking for co founder, looking for a business partner, need a business partner, equity partner, equity company, tech co-founder, technical co founder, tech partner, startup software, business startup software, build your company, startup partner
    description" />
    <meta name="description" content="StartupSoft is a tech partner with startups. We bring their service or product to life in exchange for equity. If you are looking for cofounder or a business partner - you are in the right place." />
    <meta name="google-site-verification" content="30GFHU62ZaYiCdtSA8-hfCLmbeWhDecXE5YwacCNYb4" />
    <title ng-bind="$state.current.data.pageTitle"></title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="shortcut icon" href="/Content/img/favicon.png" type="image/x-icon">
</head>
<body ng-controller="bodyCtrl as $ctrl" ng-class="{'no-padding':$state.current.name === 'home'|| $state.current.name === 'callback'}">

    <ui-view autoscroll="true">
        @RenderBody()
    </ui-view>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/main")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/angular-comp")
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/wow.min.js"></script>
    <script>
        $(window).load(function () {
            new WOW().init();
        });
    </script>
